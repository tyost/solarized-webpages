{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "moduleResolution": "node",
    "isolatedModules": false,
    "jsx": "react",
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "declaration": false,
    "noImplicitAny": false,
    "noImplicitUseStrict": false,
    "removeComments": false,
    "noLib": false,
    "preserveConstEnums": true,
    "suppressImplicitAnyIndexErrors": true,
    "outFile": "./solarized-webpages.user.js"
  },
  "filesGlob": [
    "src/Header.ts",
    "**/*.ts"
  ],
  "exclude": [
    "node_modules",
    "typings/browser",
    "typings/browser.d.ts"
  ],
  "compileOnSave": true,
  "buildOnSave": true,
  "atom": {
    "rewriteTsconfig": true
  },
  "files": [
    "src/Header.ts",
    "lib/es6-shim.d.ts",
    "src/Greasemonkey.ts",
    "src/solarized-webpages.user.ts"
  ]
}
